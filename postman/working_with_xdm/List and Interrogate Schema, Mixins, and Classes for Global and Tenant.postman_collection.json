              "listen": "test",
              "script": {
                "id": "b20de103-e3aa-4565-8b48-caa91eb8be0d",
                "exec": [
                  "var json = JSON.parse(responseBody);",
                  "postman.setEnvironmentVariable(\"ACCESS_TOKEN\", json.ACCESS_TOKEN);"
                ],
                "type": "text/javascript"
              }
            }
          ],
          "request": {
            "auth": {
              "type": "noauth"
            },
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json",
                "type": "text"
              },
              {
                "key": "Accept",
                "value": "application/json",
                "type": "text"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": ""
            },
            "url": {
              "raw": "https://{{ADOBE_LOGIN_HOST}}/ims/token/v1?client_id={{API_KEY}}&client_secret={{IMS_CLIENT_SECRET}}&code={{IMS_CLIENT_CODE}}&grant_type=authorization_code",
              "protocol": "https",
              "host": ["{{ADOBE_LOGIN_HOST}}"],
              "path": ["ims", "token", "v1"],
              "query": [
                {
                  "key": "client_id",
                  "value": "{{API_KEY}}"
                },
                {
                  "key": "client_secret",
                  "value": "{{IMS_CLIENT_SECRET}}"
                },
                {
                  "key": "code",
                  "value": "{{IMS_CLIENT_CODE}}"
                },
                {
                  "key": "grant_type",
                  "value": "authorization_code"
                }
              ]
            },
            "description": "Requires Client ID and Code"
          },
          "response": []
        },
