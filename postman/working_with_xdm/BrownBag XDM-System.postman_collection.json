		{
			"name": "1. Schema creation via Mixin",
			"item": [
				{
					"name": "Create Mixin from CUSTOM DataType",
					"event": [
						{
							"listen": "test",
							"script": {
								"id": "cc7e8748-cf1a-4173-83f3-bb4a7434e98f",
								"exec": [
									"var json = JSON.parse(responseBody);",
									"//postman.setEnvironmentVariable(\"TEST\", json.version);",
									"",
									"//console.log(json);",
									"",
									"var MIXINCUSTOMDATATYPEFULL = json['meta:altId'];",
									"postman.setEnvironmentVariable(\"MIXINCUSTOMDATATYPEFULL\", MIXINCUSTOMDATATYPEFULL);",
									"tests[\"MIXINCUSTOMDATATYPEFULL = \" + MIXINCUSTOMDATATYPEFULL] = true; ",
									"",
									"var MIXINCUSTOMDATATYPEURL = json.$id;",
									"postman.setEnvironmentVariable(\"MIXINCUSTOMDATATYPEURL\", MIXINCUSTOMDATATYPEURL);",
									"tests[\"MIXINCUSTOMDATATYPEURL = \" + MIXINCUSTOMDATATYPEURL] = true; ",
									"",
									"var SPLIT_HEADER = MIXINCUSTOMDATATYPEURL.split('/');",
									"var MIXINCUSTOMDATATYPEID = SPLIT_HEADER[5];",
									"postman.setEnvironmentVariable(\"MIXINCUSTOMDATATYPEID\", MIXINCUSTOMDATATYPEID);",
									"tests[\"MIXINCUSTOMDATATYPEID = \" + MIXINCUSTOMDATATYPEID] = true; "
								],
								"type": "text/javascript"
							}
						}
					],
					"request": {
						"method": "POST",
						"header": [
							{
								"key": "Authorization",
								"value": "Bearer {{AUTHORIZATION_TOKEN}}"
							},
							{
								"key": "x-api-key",
								"value": "{{IMS_CLIENT_ID}}"
							},
							{
								"key": "x-gw-ims-org-id",
								"value": "{{IMS_ORG_ID}}"
							},
							{
								"key": "Content-Type",
								"value": "application/json"
							}
						],
						"body": {
							"mode": "raw",
							"raw": "{\n  \"title\": \"Mixin Custom Object 4\",\n  \"type\": \"object\",\n  \"meta:extensible\": true,\n  \"meta:abstract\": true,\n  \"description\": \"Use Mixin to create Custom Schema\",\n  \"definitions\": {\n    \"customMixinSchema\": {\n      \"properties\": {\n        \"ECID\": {\n          \"title\": \"ECID\",\n          \"type\": \"string\",\n          \"description\": \"ECID\"\n        },\n        \"CRMID\": {\n          \"title\": \"CRMID\",\n          \"type\": \"string\",\n          \"description\": \"CRMID\"\n        },\n        \"Email\": {\n          \"title\": \"Email\",\n          \"type\": \"string\",\n          \"description\": \"Email\"\n        },\n        \"MarketingCode\": {\n          \"title\": \"MarketingCode\",\n          \"type\": \"string\",\n          \"description\": \"Marketing Code\"\n        }\n      }\n    }\n  },\n  \"allOf\": [\n    {\n      \"$ref\": \"https://ns.adobe.com/xdm/data/record\"\n    }\n  ]\n}"
						},
						"url": {
							"raw": "https://{{ADOBE_IO_HOST}}/data/foundation/schemaregistry/tenant/datatypes",
							"protocol": "https",
							"host": [
								"{{ADOBE_IO_HOST}}"
							],
							"path": [
								"data",
								"foundation",
								"schemaregistry",
								"tenant",
								"datatypes"
							]
						},
						"description": "Create Customer Schema (target schema) in tenant"
					},
					"response": []
				},
